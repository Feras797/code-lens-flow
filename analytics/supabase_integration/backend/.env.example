# CodeLens Analytics Backend Environment Variables
# Copy this file to .env and fill in your actual Supabase values

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-supabase-anon-key-here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=true

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Optional: Database table name (default: claude_chat_logs)
CHAT_LOGS_TABLE=claude_chat_logs
